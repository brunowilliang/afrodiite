{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "biome:format": "biome format --write",
    "biome:lint": "biome check",
    "check-types": "tsc --noEmit",
    "db:push": "drizzle-kit push --config=src/api/drizzle.config.ts",
    "db:pull": "drizzle-kit pull --config=src/api/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=src/api/drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config=src/api/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=src/api/drizzle.config.ts",
    "db:seed": "bun run src/api/database/seed.ts",
    "db:seed:reset": "bun run src/api/database/reset.ts",
    "auth:gen": "@better-auth/cli@latest generate --output src/api/database/schemas/auth.ts --config src/lib/auth/auth.server.ts --yes",
    "npx:auth:gen": "npx @better-auth/cli@latest generate --config src/lib/auth/auth.server.ts --output src/api/database/schemas/auth.ts --yes",
    "email:dev": "email dev --dir src/lib/email/templates",
    "email:test": "TEST_TO=$TEST_TO bun run scripts/send-test-email.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.896.0",
