    "@orpc/tanstack-query": "^1.9.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@react-aria/i18n": "^3.12.12",
    "@react-email/components": "^0.5.5",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.90.2",
    "@vercel/functions": "^3.1.0",
    "better-auth": "^1.3.18",
    "better-upload": "^2.0.1",
